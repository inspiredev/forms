{
  "name": "inspired-forms",
  "version": "1.0.0",
  "description": "personal node form processing",
  "main": "app.js",
  "dependencies": {
    "body-parser": "^1.15.0",
    "bootstrap-sass": "^3.3.6",
    "compression": "^1.6.1",
    "dotenv": "^2.0.0",
    "express": "~4.13.4",
    "express-handlebars": "^3.0.0",
    "level": "^1.4.0",
    "lodash": "~4.5.0",
    "morgan": "^1.7.0",
    "nodemailer": "^2.2.1",
    "nodemailer-smtp-transport": "^2.4.1",
    "passport": "~0.3.2",
    "serve-static": "^1.10.2",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "browserify": "^13.0.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.0",
    "postcss-cli": "^2.5.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "clean": "rm -rf public && mkdir public",
    "css": "node-sass sass/main.scss | postcss --use autoprefixer -o public/main.css",
    "css:watch": "node-sass --watch sass/main.scss public/main.css & postcss --use autoprefixer -o public/css/main.css public/main.css",
    "js": "browserify client/main.js -o public/main.js",
    "js:watch": "watchify client/main.js -o public/main.js -dv",
    "build": "npm run clean && npm run css && npm run js",
    "server:dev": "nodemon app.js",
    "dev": "npm run build && npm run css:watch & npm run js:watch & npm run server:dev",
    "start": "npm run build && node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tnguyen14/forms.git"
  },
  "keywords": [
    "forms",
    "node"
  ],
  "author": "Tri Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tnguyen14/forms/issues"
  }
}
